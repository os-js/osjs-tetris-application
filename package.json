{
  "name": "@osjs/tetris-application",
  "description": "OS.js Tetris Game",
  "version": "1.0.2",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint *.js",
    "build": "webpack",
    "watch": "webpack --watch",
    "prepublishOnly": "npm run eslint && rm -rf ./dist/* && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/os-js/osjs-tetris-application.git"
  },
  "files": [
    "dist/",
    "metadata.json",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "osjs"
  ],
  "author": "Anders Evenrud <andersevenrud@gmail.com>",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/os-js/osjs-tetris-application/issues"
  },
  "homepage": "https://github.com/os-js/osjs-tetris-application#readme",
  "dependencies": {
    "simple-tetris": "^1.2.0"
  },
  "devDependencies": {
    "@osjs/dev-meta": "^1.0.3"
  },
  "osjs": {
    "type": "package"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": "@osjs/eslint-config"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
